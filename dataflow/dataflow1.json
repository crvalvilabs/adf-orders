{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_raw_orders",
						"type": "DatasetReference"
					},
					"name": "OrdersRaw"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "OrderDateAndUpdateTimestamp"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> OrdersRaw",
				"OrdersRaw derive(ORDER_DATE = toDate(ORDER_DATETIME, 'dd-MMM-yy HH.mm.ss.SS')) ~> OrderDateAndUpdateTimestamp"
			]
		}
	}
}
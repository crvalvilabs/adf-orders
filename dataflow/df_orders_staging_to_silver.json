{
	"name": "df_orders_staging_to_silver",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_orders_staging",
						"type": "DatasetReference"
					},
					"name": "OrdersStaging"
				},
				{
					"dataset": {
						"referenceName": "ds_order_item_staging",
						"type": "DatasetReference"
					},
					"name": "OrdersItemsStaging"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "OrdersOrdersItemsJoin"
				},
				{
					"name": "DropColumns"
				},
				{
					"name": "SubtotalAndUpdatedTimestampOrders"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDER_ID as integer,",
				"          ORDER_DATE as date,",
				"          CUSTOMER_ID as integer,",
				"          ORDER_STATUS as string,",
				"          STORE_ID as integer,",
				"          UPDATED_TIMESTAMP as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> OrdersStaging",
				"source(output(",
				"          ORDER_ID as integer,",
				"          PRODUCT_ID as integer,",
				"          UNIT_PRICE as double,",
				"          QUANTITY as integer,",
				"          UPDATED_TIMESTAMP as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> OrdersItemsStaging",
				"OrdersStaging, OrdersItemsStaging join(OrdersStaging@ORDER_ID == OrdersItemsStaging@ORDER_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> OrdersOrdersItemsJoin",
				"OrdersOrdersItemsJoin select(mapColumn(",
				"          ORDER_ID = OrdersStaging@ORDER_ID,",
				"          ORDER_DATE,",
				"          CUSTOMER_ID,",
				"          ORDER_STATUS,",
				"          STORE_ID,",
				"          PRODUCT_ID,",
				"          UNIT_PRICE,",
				"          QUANTITY,",
				"          UPDATED_TIMESTAMP = OrdersItemsStaging@UPDATED_TIMESTAMP",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DropColumns",
				"DropColumns derive(SUBTOTAL = UNIT_PRICE * QUANTITY) ~> SubtotalAndUpdatedTimestampOrders"
			]
		}
	}
}